<h1>Listing software_serials</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @software_serials.each do |software_serial| %>
      <tr>
        <td><%= link_to 'Show', software_serial %></td>
        <td><%= link_to 'Edit', edit_software_serial_path(software_serial) %></td>
        <td><%= link_to 'Destroy', software_serial, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Software serial', new_software_serial_path %>
